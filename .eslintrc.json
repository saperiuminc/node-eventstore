{
    "env": {
        "node": true,
        "jasmine": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-prototype-builtins": "off",
      "no-extra-semi": "off",
      "no-mixed-spaces-and-tabs": "off",
      "no-async-promise-executor": "off"
    }
}
